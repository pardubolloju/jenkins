pipeline{
    agent any
    environment{
        PATH = "/usr/bin/mvn:$PATH"
    }
    stages{
        stage("Git Checkout"){
           steps{
               git 'https://github.com/pardubolloju/jenkins.git' 
           }      
        }
        stage("Maven Buil"){
            steps{
                sh "mvn clean package"
            }
        }
    
    }
}